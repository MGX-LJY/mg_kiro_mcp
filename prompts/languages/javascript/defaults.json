  "naming_conventions": {
    "files": {
      "component": "PascalCase (MyComponent.jsx)",
      "utility": "camelCase (userUtils.js)", 
      "config": "kebab-case (api-config.js)",
      "test": "*.test.js 或 *.spec.js"
    },
    "variables": {
      "constants": "UPPER_SNAKE_CASE",
      "functions": "camelCase",
      "classes": "PascalCase",
      "private": "_prefixed"
    },
    "api": {
      "routes": "/api/v1/resource",
      "methods": "RESTful (GET, POST, PUT, DELETE)",
      "response": "JSON格式统一响应"
    }
