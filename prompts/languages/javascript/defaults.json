  "project_structure": {
    "directories": [
      "src/",
      "src/components/",
      "src/utils/",
      "src/services/",
      "src/config/", 
      "src/middleware/",
      "tests/",
      "docs/",
      "public/",
      "build/"
    ],
    "config_files": [
      "package.json",
      "tsconfig.json", 
      ".eslintrc.js",
      ".prettierrc",
      "jest.config.js",
      ".env.example"
    ]
  },
